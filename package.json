{
  "name": "@universityofwarwick/id7",
  "version": "2.6.4",
  "scripts": {
    "dev": "parcel js/id7-bundle.js",
    "build": "parcel build js/id7-bundle.js",
    "test": "karma start --reporters=junit,progress js/tests/karma.conf.js",
    "start": "npm-run-all --parallel readme-md2html docs-watch docs-serve",
    "docs-serve": "bundle exec jekyll serve",
    "docs-build": "bundle exec jekyll build",
    "readme-md2html": "marked README.md > docs/_includes/README.html"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.12.1",
    "bootstrap": "3.4.1",
    "bootstrap-3-typeahead": "github:UniversityofWarwick/Bootstrap-3-Typeahead#acada2f8e5d9a70dbb37c1fd152895937a03f8d5",
    "headroom.js": "0.11.0",
    "jqdoublescroll": "github:UniversityofWarwick/jqDoubleScroll#df530aeb89a1eef243e009565f3d0743320ac753",
    "jquery": "3.4.1",
    "lodash-es": "4.17.15",
    "loglevel": "1.6.7"
  },
  "devDependencies": {
    "babel-plugin-lodash": "3.3.4",
    "less": "3.11.1",
    "parcel": "2.0.0-alpha.3.2"
  },
  "browserslist": [
    "> 1% in GB",
    "not dead"
  ],
  "browser": "dist/js/id7-bundle.js",
  "module": "dist/js/id7-module.js",
  "source": [
    "js/id7-bundle.js",
    "less/id7.less",
    "less/default-theme.less",
    "less/id6a.less"
  ],
  "targets": {
    "browser": {
      "context": "browser",
      "includeNodeModules": true,
      "outputFormat": "global",
      "isLibrary": false,
      "sourceMap": true
    },
    "module": {
      "outputFormat": "esmodule",
      "isLibrary": true
    }
  },
  "author": "University of Warwick",
  "license": "ISC",
  "homepage": "https://id7.warwick.ac.uk",
  "repository": "github:UniversityofWarwick/id7"
}
